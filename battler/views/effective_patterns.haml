- Bushido::EffectivePatterns.each.with_index do |pattern, id|
  %a{:href => "/effective_patterns?id=#{id}"}= pattern[:title]

.hr

- if @pattern
  %div= @pattern[:title]
  %div= @pattern[:comment]
  .hr
  - @frames.each do |frame|
    %div= frame.ki2_logs.last
    = FrameDecorator.new(frame).to_html_board(:small)
    .hr

